{
  "name": "docs-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npm run build:docs && docusaurus build",
    "build:docs": "npm run docusaurus clean-api-docs all && npm run docusaurus gen-api-docs all",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "typecheck-staged": "tsc-files --noEmit",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:spelling",
    "lint:js": "eslint . --ext js,jsx,ts,tsx --max-warnings=0",
    "lint:spelling": "cspell \"**\" --no-progress",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:fix": "npm run lint:js -- --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,md,mdx,ts,tsx}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": "npm run lint:fix",
    "**/*.{ts,tsx}": "npm run typecheck-staged",
    "**/*.css": "npm run lint:style",
    "**/*.{md,mdx}": "npm run lint:spelling",
    "**/*.{md,mdx,ts,js,tsx,jsx,json,txt}": "npm run format"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/plugin-google-gtag": "^2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-plugin-openapi-docs": "^1.5.1",
    "docusaurus-theme-openapi-docs": "^1.5.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^1.4.0",
    "remark-docusaurus-tabs": "^0.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cspell": "^6.18.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
